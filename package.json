{
  "name": "docs",
  "version": "2.0.0",
  "private": true,
  "license": "CC",
  "scripts": {
    "edgio:dev": "edgio dev",
    "edgio:build": "yarn rest-api docs:bundle && edgio build && yarn sitemap",
    "edgio:deploy": "yarn edgio:build && edgio deploy --skip-build",
    "analyze": "ANALYZE=true next build",
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:source": "prettier --config .prettierrc --write \"{plugins,src}/**/*.{js,ts,jsx,tsx,css}\"",
    "nit:source": "prettier --config .prettierrc --list-different \"{plugins,src}/**/*.{js,ts,jsx,tsx,css}\"",
    "prettier": "yarn format:source",
    "prettier:diff": "yarn nit:source",
    "lint-heading-ids": "node scripts/headingIdLinter.js",
    "_fix-headings": "node scripts/headingIdLinter.js --fix",
    "fix-callouts": "node scripts/headingCalloutLinter.js --fix",
    "ci-check": "npm-run-all prettier:diff --parallel lint tsc lint-heading-ids",
    "tsc": "tsc --noEmit",
    "start": "next start",
    "postinstall": "is-ci || (husky install .husky)",
    "check-all": "npm-run-all prettier lint:fix tsc",
    "clean": "rimraf node_modules && yarn",
    "update-template-tables": "python3 scripts/md-tables-to-json.py",
    "validate-links": "node scripts/validateGuideLinks.js",
    "sitemap": "next-sitemap",
    "rest-api": "yarn workspace rest-api run"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix --cache --cache-location .next/cache/eslint/",
      "yarn prettier"
    ],
    "src/templates/table_*.md": [
      "yarn update-template-tables"
    ]
  },
  "resolutions": {
    "@types/react": "^18.3.3"
  },
